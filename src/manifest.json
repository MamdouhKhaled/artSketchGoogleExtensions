{
    "manifest_version": 3,
    "name": "Art Sketch v2",
    "version": "2.0.0",
    "description": "Employee helper for my company.",

    "icons": {
        "16": "assets/icon/icon16.png",
        "48": "assets/icon/icon32.png",
        "128": "assets/icon/icon200.png"
    },
    "options_page": "options.html",
    "action": {
        "default_icon": {
            "48": "assets/icon/icon32.png"
        },
        "default_popup": "popup.html"
    },
    "chrome_url_overrides": {
        "newtab": "tabs.html"
    },
    "permissions": ["storage"]
}